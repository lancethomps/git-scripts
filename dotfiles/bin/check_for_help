#!/usr/bin/env bash
################################################################### SETUP ########################################################################
set -o errexit -o errtrace -o nounset
##################################################################################################################################################

while test "$#" -gt 0; do
  case "$1" in
    -h | --help | -help | --usage | -usage | help)
      exit 0
      ;;
    *)
      shift
      ;;
  esac
done

exit 1
