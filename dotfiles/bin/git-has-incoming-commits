#!/usr/bin/env bash
################################################################### SETUP ########################################################################
set -o errexit -o errtrace -o nounset
##################################################################################################################################################

if commits="$(git rev-list --left-right '@{upstream}...HEAD' 2> /dev/null)"; then
  for commit in $commits; do
    case "$commit" in
      "<"*)
        exit 0
        ;;
      *)
        true
        ;;
    esac
  done
fi

exit 1
